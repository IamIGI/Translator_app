    "paths": {
        "/translate": {
            "post": {
                "summary": "Translate",
                "description": "This endpoint translate from source language to target language you have defined!\nand it is designed to handle very high-volume use keeping the cost lower.",
                "operationId": "translate_translate_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/TranslateModel"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "translations": {
                                        "text": "My client speaks only French. Will you translate for me?",
                                        "translation": "Mon client ne parle que le franÃ§ais. Voulez-vous traduire pour moiÂ ?",
                                        "source": "en",
                                        "target": "fr"
                                    },
                                    "details": {}
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "APIKeyHeader": []
                    }
                ]
            }
        },
        "/language_detect": {
            "post": {
                "summary": "Detect",
                "description": "This endpoint detects the Language from the text provided!",
                "operationId": "detect_language_detect_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/DetectLanguageModel"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "example": {
                                    "language_detection": {
                                        "text": "My client speaks only French.",
                                        "language": "en"
                                    }
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "APIKeyHeader": []
                    }
                ]
            }
        },
